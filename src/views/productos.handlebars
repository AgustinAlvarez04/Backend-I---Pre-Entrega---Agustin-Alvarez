<h1>Lista de Productos</h1>

<table>
    <tr>
        <th>Titulo</th>
        <th>Descripcion</th>
        <th>Precio</th>
    </tr>
    {{#each products }}
    <tr>
        <td>{{this.title}}</td>
        <td>{{this.description}}</td>
        <td>{{this.price}}</td>
    </tr>
    {{/each}}
</table>
